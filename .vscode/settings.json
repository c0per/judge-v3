// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "formatAll.includeFileExtensions": [".ts", ".json", ".js"],
    "formatAll.excludeFolders": [
        "node_modules",
        "out",
        ".vscode-test",
        "media",
        "sandbox",
        "lib"
    ],
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "none"
}
